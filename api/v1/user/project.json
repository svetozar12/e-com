{
  "name": "protos-user",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "api",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx buf generate -o ./dist --template ../buf.gen.yaml",
        "cwd": "api/v1/user"
      },
      "outputs": ["{projectRoot}/dist"],
      "inputs": ["{projectRoot}/*.proto"]
    }
  },
  "tags": []
}
