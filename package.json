{
  "name": "@svetozar12/e-com",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "serve:all": "nx run-many --target=serve --parallel --maxParallel=100",
    "build:all": "nx run-many --target=build --parallel=3 --skip-nx-cache",
    "lint:all": "nx run-many --target=lint --skip-nx-cache",
    "test:all": "cross-env NODE_ENV=test nx run-many --target=test --exclude=netfield-web-next --skip-nx-cache",
    "postinstall": "run-script-os",
    "postinstall:win32": "./tools/scripts/postinstall.sh",
    "postinstall:linux": "chmod +x ./tools/scripts/postinstall.sh && ./tools/scripts/postinstall.sh"
  },
  "private": true,
  "dependencies": {
    "cross-env": "^7.0.3",
    "make-dir-cli": "^3.1.0",
    "mongoose": "^5.13.3",
    "run-script-os": "^1.1.6"
  },
  "devDependencies": {
    "@nx-go/nx-go": "^2.7.0",
    "@nx/eslint-plugin": "16.5.0",
    "@nx/js": "16.5.0",
    "@nx/linter": "16.5.0",
    "@nx/workspace": "16.5.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "nx": "16.5.0",
    "prettier": "^2.6.2",
    "typescript": "~5.1.3"
  }
}
