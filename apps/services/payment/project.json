{
  "name": "services-payment",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/services/payment",
  "targets": {
    "build": {
      "executor": "@nx-go/nx-go:build",
      "options": {
        "outputPath": "dist/apps/services/payment/payment",
        "main": "apps/services/payment/main.go"
      }
    },
    "serve": {
      "executor": "@nx-go/nx-go:serve",
      "options": {
        "cwd": ".",
        "main": "apps/services/payment/main.go"
      }
    },
    "test": {
      "executor": "@nx-go/nx-go:test"
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    },
    "docker-build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "cross-env-shell docker build --no-cache --force-rm -f apps/services/payment/build/Dockerfile . -t {args.registry}/e-com-microservice/payment-service:$npm_package_version"
        ]
      }
    }
  },
  "implicitDependencies": ["protos-payment"],
  "tags": []
}
