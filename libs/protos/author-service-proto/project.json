{
  "name": "protos-author-service-proto",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/protos/author-service-proto/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/protos/author-service-proto/src",
        "command": "npx proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=lib/codegen ./lib/author.v1.proto"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/protos/author-service-proto/**/*.ts"]
      }
    }
  },
  "tags": []
}
